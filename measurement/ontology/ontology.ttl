###  https://w3id.org/fedecom/measurement#alternativeID
:alternativeID rdf:type owl:DatatypeProperty ;
               rdfs:comment "Alternative identifier for a Measurement" ;
               rdfs:label "alternative ID" ;
               <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#energyAccumulated
:energyAccumulated rdf:type owl:DatatypeProperty ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf [ rdf:type rdf:List ;
                                            rdf:first 0 ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first 1 ;
                                                       rdf:rest rdf:nil
                                                     ]
                                          ]
                              ] ;
                   rdfs:comment "Boolean that establishes is the measurement value is cumulative or not." ;
                   rdfs:label "energy accumulated" ;
                   <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#measurementID
:measurementID rdf:type owl:DatatypeProperty ;
               rdfs:comment "The ID of a measurement" ;
               rdfs:label "measurement ID" ;
               <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#middlewareScalingFactor
:middlewareScalingFactor rdf:type owl:DatatypeProperty ;
                         rdfs:comment "The scaling factor used to convert the raw values (i.e. the measurements that come directly from the API in a specific unit) to the target units of the target platform." ;
                         rdfs:label "middleware scaling factor" ;
                         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#phase
:phase rdf:type owl:DatatypeProperty ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:oneOf [ rdf:type rdf:List ;
                                rdf:first 0 ;
                                rdf:rest [ rdf:type rdf:List ;
                                           rdf:first 1 ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first 2 ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first 3 ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ]
                              ]
                  ] ;
       rdfs:comment "The alternating current (AC) phase the Measurement corresponds to." ;
       rdfs:label "phase" ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#timeResolution
:timeResolution rdf:type owl:DatatypeProperty ;
                rdfs:comment "The temporal granularity at which data is captured or aggregated." ;
                rdfs:label "time resolution" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


