###  https://w3id.org/fedecom/measurement#hasAPIProvider
:hasAPIProvider rdf:type owl:ObjectProperty ;
                rdfs:domain :Measurement ;
                rdfs:range :APIProvider ;
                rdfs:comment "Relates a measurement to the entity that provides an Application Programming Interface (API) for accessing or interacting with it." ;
                rdfs:label "has API provider" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#hasDirection
:hasDirection rdf:type owl:ObjectProperty ;
              rdfs:domain :Measurement ;
              rdfs:range :MeasurementDirection ;
              rdfs:comment "Relates a measurement or flow to its directional context." ;
              rdfs:label "has direction" ;
              <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#makesMeasurement
:makesMeasurement rdf:type owl:ObjectProperty ;
                  rdfs:range :Measurement ;
                  rdfs:comment "Links an agent (e.g., a sensor, device, or system) to the measurement it produces." ;
                  rdfs:label "makes measurement" ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#measurementBy
:measurementBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Measurement ;
               rdfs:comment "Relates a measurement to the agent or instrument that performed or generated it." ;
               rdfs:label "measurement by" ;
               <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#measuresOn
:measuresOn rdf:type owl:ObjectProperty ;
            rdfs:domain :Measurement ;
            rdfs:comment "Links a measurement to the entity or object on which the measurement was taken." ;
            rdfs:label "measures on" ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#measuresProperty
:measuresProperty rdf:type owl:ObjectProperty ;
                  rdfs:domain :Measurement ;
                  rdfs:range :Property ;
                  rdfs:comment "Relates a measurement to the specific property or characteristic being observed." ;
                  rdfs:label "measures property" ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


