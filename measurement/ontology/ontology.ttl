#################################################################

###  https://w3id.org/fedecom/measurement#APIProvider
:APIProvider rdf:type owl:Class ;
             rdfs:comment "The provider of the API that exposes the measurements for a Pilot." ;
             rdfs:label "API provider" ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
             <http://www.w3.org/2004/02/skos/core#example> "AEM, FLEXO" .


###  https://w3id.org/fedecom/measurement#Measurement
:Measurement rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasAPIProvider ;
                               owl:someValuesFrom :APIProvider
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasDirection ;
                               owl:someValuesFrom :MeasurementDirection
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :measuresProperty ;
                               owl:someValuesFrom :Property
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :APIParameter ;
                               owl:allValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :energyAccumulated ;
                               owl:allValuesFrom xsd:boolean
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :measurementID ;
                               owl:allValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :middlewareScalingFactor ;
                               owl:allValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :phase ;
                               owl:allValuesFrom xsd:integer
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :timeResolution ;
                               owl:allValuesFrom xsd:string
                             ] ;
             rdfs:comment "A quantitative observation or value obtained from a sensor, instrument, or calculation." ;
             rdfs:label "Measurement" ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/measurement#MeasurementDirection
:MeasurementDirection rdf:type owl:Class ;
                      rdfs:comment "It determines how the measured quantity flows or is interpreted in terms of directionality." ;
                      rdfs:label "Measurement direction" ;
                      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                      <http://www.w3.org/2004/02/skos/core#example> "import, export, signed" .


###  https://w3id.org/fedecom/measurement#Property
:Property rdf:type owl:Class ;
          rdfs:comment "Represents the attribute or characteristic measured in a measurement." ;
          rdfs:label "Property" ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
          <http://www.w3.org/2004/02/skos/core#example> "active energy, humidity, current" .


