#################################################################
#    Data properties
#################################################################

###  http://schema.org/name
<http://schema.org/name> rdf:type owl:DatatypeProperty .


###  https://w3id.org/fedecom/energyasset#connectionType
:connectionType rdf:type owl:DatatypeProperty ;
                rdfs:domain :StorageTank ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "parallel" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "series" ;
                                                    rdf:rest rdf:nil
                                                  ]
                                       ]
                           ] ;
                rdfs:comment """Specifies the type of connection configuration for an asset.
Allowed values:

parallel – Components are connected in parallel, sharing input and output points to maintain equal voltage.
series – Components are connected in sequence, so the same current flows through each component.""" ;
                rdfs:label "connection type" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/energyasset#hasCapacityRange
:hasCapacityRange rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:comment "Defines the range of capacity values for an asset (minimum and maximum)." ;
                  rdfs:label "capacity range" ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/energyasset#hasMaximumPowerRating
:hasMaximumPowerRating rdf:type owl:DatatypeProperty ;
