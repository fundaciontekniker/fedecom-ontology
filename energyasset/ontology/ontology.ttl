###  https://w3id.org/fedecom/energyasset#Boiler
:Boiler rdf:type owl:Class ;
        rdfs:subClassOf :HVAC ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCapacity ;
                          owl:someValuesFrom :Capacity
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasEfficiency ;
                          owl:someValuesFrom :Efficiency
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasResistance ;
                          owl:someValuesFrom :Resistance
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :isGenerationEquipment ;
                          owl:someValuesFrom xsd:boolean
                        ] ;
        rdfs:comment "A device that heats water or produces steam for heating or power generation." ;
        rdfs:label "Boiler" ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


