#################################################################
#    Classes
#################################################################

###  http://w3id.org/fedecom/battery#Battery
<http://w3id.org/fedecom/battery#Battery> rdf:type owl:Class ;
                                          rdfs:subClassOf :Device .


###  https://w3id.org/fedecom/energyasset#AirHandlingUnit
:AirHandlingUnit rdf:type owl:Class ;
                 rdfs:subClassOf :HVAC ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasCapacity ;
                                   owl:someValuesFrom :Capacity
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasEfficiency ;
                                   owl:someValuesFrom :Efficiency
                                 ] ;
                 rdfs:comment "A device that conditions and circulates air as part of an HVAC system, typically including fans, filters, and heat exchangers." ;
                 rdfs:label "Air Handling Unit" ;
                 <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/energyasset#Azimuth
:Azimuth rdf:type owl:Class ;
         rdfs:subClassOf :Characteristic ;
         rdfs:comment "The angular measurement in the horizontal plane between a reference direction (usually true north) and the direction of a solar panel." ;
         rdfs:label "Azimuth" ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/energyasset#BiomassBoiler
