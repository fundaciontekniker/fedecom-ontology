    <!-- https://w3id.org/fedecom/battery#Battery -->


    <owl:Class rdf:about="https://w3id.org/fedecom/battery#Battery">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasChargeCurrent"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#ChargeCurrent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasChargePower"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#ChargePower"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasDischargeCurrent"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#DischargeCurrent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasDischargePower"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#DischargePower"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasLifetime"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#BatteryLifetime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasLossRate"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#LossRate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasManufacturer"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#Manufacturer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasModel"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#Model"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasOpenCircuitVoltageOverDOD"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#OpenCircuitVoltageOverDOD"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasOpenCircuitVoltageOverSOC"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#OpenCircuitVoltageOverSOC"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasPowerDelivery"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#PowerDelivery"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasSOC"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#SOC"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasStorageLevel"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#StorageLevel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/battery#hasVoltage"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/battery#Voltage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A device that stores chemical energy and converts it into electrical energy through electrochemical reactions.</rdfs:comment>
        <rdfs:label>Battery</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/battery#BatteryChemistryType -->


    <owl:Class rdf:about="https://w3id.org/fedecom/battery#BatteryChemistryType">
        <rdfs:comment>The specific chemical composition and technology used in a battery.</rdfs:comment>
        <rdfs:label>Battery chemistry type</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Lithium-ion, Lead-acid, Nickel-metal hydride</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/battery#BatteryLifetime -->


    <owl:Class rdf:about="https://w3id.org/fedecom/battery#BatteryLifetime">
        <rdfs:comment>The total period a battery can function effectively before its capacity drops below a usable threshold, often measured in cycles or years.</rdfs:comment>
        <rdfs:label>Battery lifetime</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/battery#Capacity -->


    <owl:Class rdf:about="https://w3id.org/fedecom/battery#Capacity">
        <rdfs:comment>The amount of electric charge a battery can store.</rdfs:comment>
        <rdfs:label>Capacity</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/battery#ChargeCapacity -->


    <owl:Class rdf:about="https://w3id.org/fedecom/battery#ChargeCapacity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/battery#Capacity"/>
        <rdfs:comment>The maximum amount of charge that can be stored during charging, usually equal to the battery’s rated capacity.</rdfs:comment>
        <rdfs:label>Charge capacity</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/battery#ChargeCurrent -->


    <owl:Class rdf:about="https://w3id.org/fedecom/battery#ChargeCurrent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/battery#Current"/>
        <rdfs:comment>The rate at which current flows into the battery during charging.</rdfs:comment>
        <rdfs:label>Charge current</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
