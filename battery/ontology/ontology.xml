    <!-- https://w3id.org/fedecom/battery#hasCharacteristic -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasCharacteristic">
        <rdfs:comment>Links a battery to a general feature or property that describes its nature, such as chemistry type, capacity, or performance attributes.</rdfs:comment>
        <rdfs:label>has characteristic</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasChargeCurrent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasChargeCurrent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#ChargeCurrent"/>
        <rdfs:comment>Relates a battery to the value of electric current supplied during charging.</rdfs:comment>
        <rdfs:label>has charge current</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasChargePower -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasChargePower">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#ChargePower"/>
        <rdfs:comment>Relates a battery to the amount of power delivered during charging.</rdfs:comment>
        <rdfs:label>has charge power</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasDischargeCurrent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasDischargeCurrent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#DischargeCurrent"/>
        <rdfs:comment>Relates a battery to the value of electric current drawn during discharge.</rdfs:comment>
        <rdfs:label>has discharge current</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasDischargePower -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasDischargePower">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#DischargePower"/>
        <rdfs:comment>Relates a battery to the amount of power output during discharge.</rdfs:comment>
        <rdfs:label>has discharge power</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasLifetime -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasLifetime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#BatteryLifetime"/>
        <rdfs:comment>Relates a battery to its expected operational duration.</rdfs:comment>
        <rdfs:label>has lifetime</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasLossRate -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasLossRate">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#LossRate"/>
        <rdfs:comment>Relates a battery to the rate at which it loses energy or capacity over time.</rdfs:comment>
        <rdfs:label>has loss rate</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasManufacturer -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasManufacturer">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#Manufacturer"/>
        <rdfs:comment>Links a battery to the entity (company or organization) that produced it.</rdfs:comment>
        <rdfs:label>has manufacturer</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasModel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasModel">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#Model"/>
        <rdfs:comment>Links a battery to its specific product designation or identifier.</rdfs:comment>
        <rdfs:label>has model</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasOpenCircuitVoltage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasOpenCircuitVoltage">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:comment>Relates a battery to its voltage when not connected to any load or charging source.</rdfs:comment>
        <rdfs:label>has open circuit voltage</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasOpenCircuitVoltageOverDOD -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasOpenCircuitVoltageOverDOD">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasOpenCircuitVoltage"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#OpenCircuitVoltageOverDOD"/>
        <rdfs:comment>Relates a battery to the functional relationship between open circuit voltage and depth of discharge.</rdfs:comment>
        <rdfs:label>has open circuit voltage over Depth of Discharge</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasOpenCircuitVoltageOverSOC -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasOpenCircuitVoltageOverSOC">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasOpenCircuitVoltage"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#OpenCircuitVoltageOverSOC"/>
        <rdfs:comment>Relates a battery to the functional relationship between open circuit voltage and state of charge.</rdfs:comment>
        <rdfs:label>has open circuit voltage over State of Charge</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasPowerDelivery -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasPowerDelivery">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#PowerDelivery"/>
        <rdfs:comment>Relates a battery to its ability to supply power to a load.</rdfs:comment>
        <rdfs:label>has power delivery</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasSOC -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasSOC">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#SOC"/>
        <rdfs:comment>Relates a battery to its current level of charge.</rdfs:comment>
        <rdfs:label>has state of charge</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasStorageLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasStorageLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#StorageLevel"/>
        <rdfs:comment>Relates a battery to the amount of energy stored at a given time.</rdfs:comment>
        <rdfs:label>has storage level</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/battery#hasVoltage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/battery#hasVoltage">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/battery#hasCharacteristic"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/battery#Battery"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/battery#Voltage"/>
        <rdfs:comment>Relates a battery to its operating electrical potential difference.</rdfs:comment>
        <rdfs:label>has voltage</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


