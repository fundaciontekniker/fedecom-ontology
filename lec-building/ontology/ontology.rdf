    <!-- https://w3id.org/fedecom/lec-building#hasArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasArea">
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#BuildingArea"/>
        <rdfs:comment>Links an entity to its corresponding Area value.</rdfs:comment>
        <rdfs:label>has area</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasBuildingTypology -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasBuildingTypology">
        <rdfs:label>has building typology</rdfs:label>
        <ns1:term_status>in study</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasCircuit -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasCircuit">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#Circuit"/>
        <rdfs:comment>Connects a Building entity to an electrical circuit that supplies or distributes power within the building.</rdfs:comment>
        <rdfs:label>has circuit</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasConsumption -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasConsumption">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:label>has consumption</rdfs:label>
        <ns1:term_status>in study</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasElement">
        <rdfs:label>has element</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasEnergyAsset -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasEnergyAsset">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/lec-building#hasElement"/>
        <rdfs:comment>Links an entity (usually Building) to an asset that generates, stores, or manages energy (e.g., solar panel, battery).</rdfs:comment>
        <rdfs:label>has energy asset</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasEquipment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasEquipment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/fedecom/lec-building#hasElement"/>
        <rdfs:comment>Connects an entity (usually Building) to devices or machinery used for specific operations or processes.</rdfs:comment>
        <rdfs:label>has equipment</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasFloor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasFloor">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#Floor"/>
        <rdfs:comment>Relates a Building with the Floors that it is composed of.</rdfs:comment>
        <rdfs:label>has floor</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasLocation">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#LEC"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#Location"/>
        <rdfs:comment>Associates an entity with a geographic or spatial position.</rdfs:comment>
        <rdfs:label>has location</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasParticipant -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasParticipant">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#Site"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>Links a Site with the Buildings it is composed of.</rdfs:comment>
        <rdfs:label>has participant</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasRoom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasRoom">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#Floor"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#Room"/>
        <rdfs:comment>Links a Floor with the Rooms included in it.</rdfs:comment>
        <rdfs:label>has room</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#hasSite -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fedecom/lec-building#hasSite">
        <rdfs:domain rdf:resource="https://w3id.org/fedecom/lec-building#LEC"/>
        <rdfs:range rdf:resource="https://w3id.org/fedecom/lec-building#Site"/>
        <rdfs:comment>Connects a LEC with the Sites it includes.</rdfs:comment>
        <rdfs:label>has site</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


