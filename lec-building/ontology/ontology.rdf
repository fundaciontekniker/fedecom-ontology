    <!-- https://w3id.org/fedecom/lec-building#AssemblyBuilding -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#AssemblyBuilding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>&quot;Assembly Group A occupancy includes, among othersm the use of a building or structure, or a portion thereof, for the gathering of persons for purposes such as civic, social or religious functions; recreation, food or drink consumption or awaiting transportation&quot; (2024 IBC)</rdfs:comment>
        <rdfs:isDefinedBy>2024 International Building Code (Ver. Aug 2025)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use</rdfs:isDefinedBy>
        <rdfs:label>Assembly Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Motion picture theaters, Symphony and concert halls; Banquet halls, Restaurants, cafeterias and similar dining facilities (including associated commercial kitchens); Amusement arcades, Art galleries; Swimming pools, Tennis courts; Amusement park structures, Stadiums</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#Building -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#Building">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasArea"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#BuildingArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasBuildingTypology"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#BuildingTypology"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasCircuit"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#Circuit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasFloor"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#Floor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasLocation"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasRoom"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#Room"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#BuildingArea -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#BuildingArea">
        <rdfs:comment>Total floor space of a building measured at all levels, including walls and structural elements, typically expressed in square units (e.g., square meters or square feet).</rdfs:comment>
        <rdfs:label>Building Area</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#BuildingTypology -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#BuildingTypology"/>
    


    <!-- https://w3id.org/fedecom/lec-building#BusinessBuilding -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#BusinessBuilding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>&quot;Business Group B occupancy, includes, among others, the use of a building or structure, or a portion thereof, for office, professional or service-type transactions, including storage of records and accounts.&quot; (2024 IDC)</rdfs:comment>
        <rdfs:isDefinedBy>2024 International Building Code (Ver. Aug 2025)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use</rdfs:isDefinedBy>
        <rdfs:label>Business Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Airport traffic control towers, Ambulatory care facilities, Animal hospitals, kennels and pounds, Banks</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#Circuit -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#Circuit">
        <rdfs:comment>Electrical distribution system within a building.</rdfs:comment>
        <rdfs:label>Circuit</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#EducationalBuilding -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#EducationalBuilding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>&quot;Educational Group E occupancy, includes, among others, the use of a building or structure, or a portion thereof, by six or more persons at any one time for educational purposes through the 12th grade.&quot; (2024 IDC)</rdfs:comment>
        <rdfs:isDefinedBy>2024 International Building Code (Ver. Aug 2025)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use</rdfs:isDefinedBy>
        <rdfs:label>Educational Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Schools</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#FactoryIndustrialBuilding -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#FactoryIndustrialBuilding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>&quot;Factory Group F occupancy, includes, among others, the use of a building or structure, or a portion thereof, for assembling, , disassembling, fabricating, finishing, manufacturing, packaging, repair or processing operations that are not as a Group H hazardous or Group S storage occupancy.&quot; (2024 IDC)</rdfs:comment>
        <rdfs:isDefinedBy>2024 International Building Code (Ver. Aug 2025)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use</rdfs:isDefinedBy>
        <rdfs:label>Factory Industrial Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Appliances, Athletic equipment, Automobiles and other motor vehicles, Bakeries; Beverages: up to and including 20 percent alcohol content, Brick and masonry, Ceramic products, Foundries</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#Floor -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#Floor">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasRoom"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/fedecom/lec-building#Room"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A horizontal level within a building, typically used for occupancy or functional spaces.</rdfs:comment>
        <rdfs:label>Floor</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#HighHazardBuilding -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#HighHazardBuilding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>&quot;High-Hazard Group H occupancy, includes, among others, the use of a building or structure, or a portion thereof, that involves the manufacturing, processing, generation or storage of materials that constitute a physical or health hazard in quantities in excess of those allowed in control aread complying with Section 414 [of 2024 IDC]&quot; (2024 IDC)</rdfs:comment>
        <rdfs:isDefinedBy>2024 International Building Code (Ver. Aug 2025)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use</rdfs:isDefinedBy>
        <rdfs:label>High Hazard Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Buildings containing hazardous materials such as: Consumer fireworks, Detonable pyrophoric materials, Oxidizers</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#InstitutionalBuilding -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#InstitutionalBuilding">
        <rdfs:subClassOf rdf:resource="https://w3id.org/fedecom/lec-building#Building"/>
        <rdfs:comment>&quot;Institutional Group I occupancy, includes, among others, the use of a building or structure, or a portion thereof, in which care or supervision is provided to persons who are or are incapable of self-preservation without physical assistance or in which persons are detained for penal or correctional purposed or in which the liberty of the occupants is restricted.&quot; (2024 IDC)</rdfs:comment>
        <rdfs:isDefinedBy>2024 International Building Code (Ver. Aug 2025)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use</rdfs:isDefinedBy>
        <rdfs:label>Institutional Building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example>Alcohol and drug centers, Assisted living facilities; Hospitals, Nursing homes, Psychiatric hospitals; Correctional centers, Detention centers, Jails; Adult day care, Child day care</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/fedecom/lec-building#LEC -->

    <owl:Class rdf:about="https://w3id.org/fedecom/lec-building#LEC">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/fedecom/lec-building#hasSite"/>
