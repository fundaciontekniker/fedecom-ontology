        <rdfs:comment>Number of children occupying a building.</rdfs:comment>
        <rdfs:label>children</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#floorNumber -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fedecom/lec-building#floorNumber">
        <rdfs:comment>Number of floors of a building.</rdfs:comment>
        <rdfs:label>floor number</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#notes -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fedecom/lec-building#notes">
        <rdfs:comment>Additional notes to a specific entity.</rdfs:comment>
        <rdfs:label>notes</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#occupation -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fedecom/lec-building#occupation">
        <rdfs:comment>Number of people occupying a building.</rdfs:comment>
        <rdfs:label>occupation</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fedecom/lec-building#yearOfConstruction -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fedecom/lec-building#yearOfConstruction">
        <rdfs:comment>The year a building was built.</rdfs:comment>
        <rdfs:label>year of construction</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:DatatypeProperty>
    


