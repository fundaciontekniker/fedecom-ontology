        <ns:license>http://purl.org/NET/rdflicense/cc-by4.0</ns:license>
        <terms:contributor>Ignacio Lázaro (Tekniker) &lt;ignacio.lazaro@tekniker.es&gt;
Andoni Osorio (Tekniker) &lt;andoni.osorio@tekniker.es&gt;
Marko Jelić (Institut Mihajlo Pupin) &lt;marko.jelic@pupin.rs&gt;
Spyros Tzavikas (Grid Singularity) &lt;spyros@gridsingularity.com&gt;</terms:contributor>
        <terms:created></terms:created>
        <terms:creator>Cristina Aceta (Tekniker) &lt;cristina.aceta@tekniker.es&gt;</terms:creator>
        <terms:description>This ontology module includes the necessary classes and properties to characterize energy communities in terms of the buildings included in them.</terms:description>
        <terms:title>LEC-Building: Energy Communities and Buildings Characterization module for the FEDECOM Ontology</terms:title>
        <vann:preferredNamespacePrefix>lec-building</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/fedecom/lec-building#</vann:preferredNamespaceUri>
        <owl:versionInfo></owl:versionInfo>
    </owl:Ontology>
    


