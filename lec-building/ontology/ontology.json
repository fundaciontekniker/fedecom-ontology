  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#EducationalBuilding",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "\"Educational Group E occupancy, includes, among others, the use of a building or structure, or a portion thereof, by six or more persons at any one time for educational purposes through the 12th grade.\" (2024 IDC)"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "2024 International Building Code (Ver. Aug 2025)"
  }, {
    "@value" : "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Educational Building"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/fedecom/lec-building#Building"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Schools"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#FactoryIndustrialBuilding",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "\"Factory Group F occupancy, includes, among others, the use of a building or structure, or a portion thereof, for assembling, , disassembling, fabricating, finishing, manufacturing, packaging, repair or processing operations that are not as a Group H hazardous or Group S storage occupancy.\" (2024 IDC)"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "2024 International Building Code (Ver. Aug 2025)"
  }, {
    "@value" : "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Factory Industrial Building"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/fedecom/lec-building#Building"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Appliances, Athletic equipment, Automobiles and other motor vehicles, Bakeries; Beverages: up to and including 20 percent alcohol content, Brick and masonry, Ceramic products, Foundries"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#Floor",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A horizontal level within a building, typically used for occupancy or functional spaces."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Floor"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:genid7"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#HighHazardBuilding",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "\"High-Hazard Group H occupancy, includes, among others, the use of a building or structure, or a portion thereof, that involves the manufacturing, processing, generation or storage of materials that constitute a physical or health hazard in quantities in excess of those allowed in control aread complying with Section 414 [of 2024 IDC]\" (2024 IDC)"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "2024 International Building Code (Ver. Aug 2025)"
  }, {
    "@value" : "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "High Hazard Building"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/fedecom/lec-building#Building"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Buildings containing hazardous materials such as: Consumer fireworks, Detonable pyrophoric materials, Oxidizers"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#InstitutionalBuilding",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "\"Institutional Group I occupancy, includes, among others, the use of a building or structure, or a portion thereof, in which care or supervision is provided to persons who are or are incapable of self-preservation without physical assistance or in which persons are detained for penal or correctional purposed or in which the liberty of the occupants is restricted.\" (2024 IDC)"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "2024 International Building Code (Ver. Aug 2025)"
  }, {
    "@value" : "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Institutional Building"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/fedecom/lec-building#Building"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Alcohol and drug centers, Assisted living facilities; Hospitals, Nursing homes, Psychiatric hospitals; Correctional centers, Detention centers, Jails; Adult day care, Child day care"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#LEC",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A group of local actors (citizens, businesses, or organizations) collaborating to produce, share, and manage energy—often renewable—within a defined area."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "LEC"
  }, {
    "@value" : "Local Energy Community"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:genid8"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@value" : "stable"
  } ]
}, {
  "@id" : "https://w3id.org/fedecom/lec-building#Location",
