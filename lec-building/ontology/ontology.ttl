@prefix : <https://w3id.org/fedecom/lec-building#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/fedecom/lec-building> .

<https://w3id.org/fedecom/lec-building> rdf:type owl:Ontology ;
                                         owl:imports <https://w3id.org/fedecom/units> ;
                                         <http://creativecommons.org/ns#license> "http://purl.org/NET/rdflicense/cc-by4.0" ;
                                         <http://purl.org/dc/terms/contributor> """Ignacio Lázaro (Tekniker) <ignacio.lazaro@tekniker.es>
Andoni Osorio (Tekniker) <andoni.osorio@tekniker.es>
Marko Jelić (Institut Mihajlo Pupin) <marko.jelic@pupin.rs>
Spyros Tzavikas (Grid Singularity) <spyros@gridsingularity.com>""" ;
                                         <http://purl.org/dc/terms/created> "" ;
                                         <http://purl.org/dc/terms/creator> "Cristina Aceta (Tekniker) <cristina.aceta@tekniker.es>" ;
                                         <http://purl.org/dc/terms/description> "This ontology module includes the necessary classes and properties to characterize energy communities in terms of the buildings included in them." ;
                                         <http://purl.org/dc/terms/title> "LEC-Building: Energy Communities and Buildings Characterization module for the FEDECOM Ontology" ;
                                         <http://purl.org/vocab/vann/preferredNamespacePrefix> "lec-building" ;
                                         <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/fedecom/lec-building#" ;
                                         owl:versionInfo "" .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
<http://creativecommons.org/ns#license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/06/sw-vocab-status/ns#term_status
<http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/fedecom/lec-building#hasArea
:hasArea rdf:type owl:ObjectProperty ;
         rdfs:range :BuildingArea ;
         rdfs:comment "Links an entity to its corresponding Area value." ;
         rdfs:label "has area" ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasBuildingTypology
:hasBuildingTypology rdf:type owl:ObjectProperty ;
                     rdfs:label "has building typology" ;
                     <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "in study" .


###  https://w3id.org/fedecom/lec-building#hasCircuit
:hasCircuit rdf:type owl:ObjectProperty ;
            rdfs:domain :Building ;
            rdfs:range :Circuit ;
            rdfs:comment "Connects a Building entity to an electrical circuit that supplies or distributes power within the building." ;
            rdfs:label "has circuit" ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasConsumption
:hasConsumption rdf:type owl:ObjectProperty ;
                rdfs:domain :Building ;
                rdfs:label "has consumption" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "in study" .


###  https://w3id.org/fedecom/lec-building#hasElement
:hasElement rdf:type owl:ObjectProperty ;
            rdfs:label "has element" ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasEnergyAsset
:hasEnergyAsset rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasElement ;
                rdfs:comment "Links an entity (usually Building) to an asset that generates, stores, or manages energy (e.g., solar panel, battery)." ;
                rdfs:label "has energy asset" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasEquipment
:hasEquipment rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasElement ;
              rdfs:comment "Connects an entity (usually Building) to devices or machinery used for specific operations or processes." ;
              rdfs:label "has equipment" ;
              <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasFloor
:hasFloor rdf:type owl:ObjectProperty ;
          rdfs:domain :Building ;
          rdfs:range :Floor ;
          rdfs:comment "Relates a Building with the Floors that it is composed of." ;
          rdfs:label "has floor" ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain :Building ,
                         :LEC ;
             rdfs:range :Location ;
             rdfs:comment "Associates an entity with a geographic or spatial position." ;
             rdfs:label "has location" ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                rdfs:domain :Site ;
                rdfs:range :Building ;
                rdfs:comment "Links a Site with the Buildings it is composed of." ;
                rdfs:label "has participant" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasRoom
:hasRoom rdf:type owl:ObjectProperty ;
         rdfs:domain :Floor ;
         rdfs:range :Room ;
         rdfs:comment "Links a Floor with the Rooms included in it." ;
         rdfs:label "has room" ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#hasSite
:hasSite rdf:type owl:ObjectProperty ;
         rdfs:domain :LEC ;
         rdfs:range :Site ;
         rdfs:comment "Connects a LEC with the Sites it includes." ;
         rdfs:label "has site" ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


#################################################################
#    Data properties
#################################################################

###  http://schema.org/name
<http://schema.org/name> rdf:type owl:DatatypeProperty ;
                         rdfs:comment "Name or identifier for an entity." ;
                         rdfs:label "name" ;
                         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdf:type owl:DatatypeProperty .


###  https://w3id.org/fedecom/lec-building#UUID
:UUID rdf:type owl:DatatypeProperty ;
      rdfs:comment "UUID of an entity." ;
      rdfs:label "UUID" ;
      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#children
:children rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :occupation ;
          rdfs:comment "Number of children occupying a building." ;
          rdfs:label "children" ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#floorNumber
:floorNumber rdf:type owl:DatatypeProperty ;
             rdfs:comment "Number of floors of a building." ;
             rdfs:label "floor number" ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#notes
:notes rdf:type owl:DatatypeProperty ;
       rdfs:comment "Additional notes to a specific entity." ;
       rdfs:label "notes" ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#occupation
:occupation rdf:type owl:DatatypeProperty ;
            rdfs:comment "Number of people occupying a building." ;
            rdfs:label "occupation" ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#yearOfConstruction
:yearOfConstruction rdf:type owl:DatatypeProperty ;
                    rdfs:comment "The year a building was built." ;
                    rdfs:label "year of construction" ;
                    <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/fedecom/lec-building#AssemblyBuilding
:AssemblyBuilding rdf:type owl:Class ;
                  rdfs:subClassOf :Building ;
                  rdfs:comment "\"Assembly Group A occupancy includes, among othersm the use of a building or structure, or a portion thereof, for the gathering of persons for purposes such as civic, social or religious functions; recreation, food or drink consumption or awaiting transportation\" (2024 IBC)" ;
                  rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                   "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                  rdfs:label "Assembly Building" ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                  <http://www.w3.org/2004/02/skos/core#example> "Motion picture theaters, Symphony and concert halls; Banquet halls, Restaurants, cafeterias and similar dining facilities (including associated commercial kitchens); Amusement arcades, Art galleries; Swimming pools, Tennis courts; Amusement park structures, Stadiums" .


###  https://w3id.org/fedecom/lec-building#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasArea ;
                            owl:someValuesFrom :BuildingArea
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasBuildingTypology ;
                            owl:someValuesFrom :BuildingTypology
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasCircuit ;
                            owl:someValuesFrom :Circuit
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasFloor ;
                            owl:someValuesFrom :Floor
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasLocation ;
                            owl:someValuesFrom :Location
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRoom ;
                            owl:someValuesFrom :Room
                          ] ;
          rdfs:label "Building" ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#BuildingArea
:BuildingArea rdf:type owl:Class ;
              rdfs:comment "Total floor space of a building measured at all levels, including walls and structural elements, typically expressed in square units (e.g., square meters or square feet)." ;
              rdfs:label "Building Area" ;
              <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#BuildingTypology
:BuildingTypology rdf:type owl:Class .


###  https://w3id.org/fedecom/lec-building#BusinessBuilding
:BusinessBuilding rdf:type owl:Class ;
                  rdfs:subClassOf :Building ;
                  rdfs:comment "\"Business Group B occupancy, includes, among others, the use of a building or structure, or a portion thereof, for office, professional or service-type transactions, including storage of records and accounts.\" (2024 IDC)" ;
                  rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                   "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                  rdfs:label "Business Building" ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                  <http://www.w3.org/2004/02/skos/core#example> "Airport traffic control towers, Ambulatory care facilities, Animal hospitals, kennels and pounds, Banks" .


###  https://w3id.org/fedecom/lec-building#Circuit
:Circuit rdf:type owl:Class ;
         rdfs:comment "Electrical distribution system within a building." ;
         rdfs:label "Circuit" ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#EducationalBuilding
:EducationalBuilding rdf:type owl:Class ;
                     rdfs:subClassOf :Building ;
                     rdfs:comment "\"Educational Group E occupancy, includes, among others, the use of a building or structure, or a portion thereof, by six or more persons at any one time for educational purposes through the 12th grade.\" (2024 IDC)" ;
                     rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                      "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                     rdfs:label "Educational Building" ;
                     <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                     <http://www.w3.org/2004/02/skos/core#example> "Schools" .


###  https://w3id.org/fedecom/lec-building#FactoryIndustrialBuilding
:FactoryIndustrialBuilding rdf:type owl:Class ;
                           rdfs:subClassOf :Building ;
                           rdfs:comment "\"Factory Group F occupancy, includes, among others, the use of a building or structure, or a portion thereof, for assembling, , disassembling, fabricating, finishing, manufacturing, packaging, repair or processing operations that are not as a Group H hazardous or Group S storage occupancy.\" (2024 IDC)" ;
                           rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                            "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                           rdfs:label "Factory Industrial Building" ;
                           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                           <http://www.w3.org/2004/02/skos/core#example> "Appliances, Athletic equipment, Automobiles and other motor vehicles, Bakeries; Beverages: up to and including 20 percent alcohol content, Brick and masonry, Ceramic products, Foundries" .


###  https://w3id.org/fedecom/lec-building#Floor
:Floor rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasRoom ;
                         owl:someValuesFrom :Room
                       ] ;
       rdfs:comment "A horizontal level within a building, typically used for occupancy or functional spaces." ;
       rdfs:label "Floor" ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#HighHazardBuilding
:HighHazardBuilding rdf:type owl:Class ;
                    rdfs:subClassOf :Building ;
                    rdfs:comment "\"High-Hazard Group H occupancy, includes, among others, the use of a building or structure, or a portion thereof, that involves the manufacturing, processing, generation or storage of materials that constitute a physical or health hazard in quantities in excess of those allowed in control aread complying with Section 414 [of 2024 IDC]\" (2024 IDC)" ;
                    rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                     "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                    rdfs:label "High Hazard Building" ;
                    <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                    <http://www.w3.org/2004/02/skos/core#example> "Buildings containing hazardous materials such as: Consumer fireworks, Detonable pyrophoric materials, Oxidizers" .


###  https://w3id.org/fedecom/lec-building#InstitutionalBuilding
:InstitutionalBuilding rdf:type owl:Class ;
                       rdfs:subClassOf :Building ;
                       rdfs:comment "\"Institutional Group I occupancy, includes, among others, the use of a building or structure, or a portion thereof, in which care or supervision is provided to persons who are or are incapable of self-preservation without physical assistance or in which persons are detained for penal or correctional purposed or in which the liberty of the occupants is restricted.\" (2024 IDC)" ;
                       rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                        "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                       rdfs:label "Institutional Building" ;
                       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                       <http://www.w3.org/2004/02/skos/core#example> "Alcohol and drug centers, Assisted living facilities; Hospitals, Nursing homes, Psychiatric hospitals; Correctional centers, Detention centers, Jails; Adult day care, Child day care" .


###  https://w3id.org/fedecom/lec-building#LEC
:LEC rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasSite ;
                       owl:someValuesFrom :Site
                     ] ;
     rdfs:comment "A group of local actors (citizens, businesses, or organizations) collaborating to produce, share, and manage energy—often renewable—within a defined area." ;
     rdfs:label "LEC" ,
                "Local Energy Community" ;
     <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#Location
:Location rdf:type owl:Class ;
          rdfs:comment "A specific point or area identified by coordinates or address, representing where an object or activity is situated." ;
          rdfs:label "Location" ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#MercantileBuilding
:MercantileBuilding rdf:type owl:Class ;
                    rdfs:subClassOf :Building ;
                    rdfs:comment "\"Mercantile Group M occupancy, includes, among others, the use of a building or structure, or a portion thereof, for the display and sale of merchandise, and involves stocks of goods, wares or merchandise incidental to such purposes and where the public has access.\" (2024 IDC)" ;
                    rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                     "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                    rdfs:label "Mercantile Building" ;
                    <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                    <http://www.w3.org/2004/02/skos/core#example> "Department stores, Drug stores, Greenhouses for display and sale of plants that provide public access, Markets" .


###  https://w3id.org/fedecom/lec-building#PrimaryCircuit
:PrimaryCircuit rdf:type owl:Class ;
                rdfs:subClassOf :Circuit ;
                rdfs:comment "Main feed from the transformer or utility source to the building’s main distribution board or primary distribution box—usually at medium voltage (e.g., 0.4 kV)." ;
                rdfs:label "Primary Circuit" ;
                <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#ResidentialBuilding
:ResidentialBuilding rdf:type owl:Class ;
                     rdfs:subClassOf :Building ;
                     rdfs:comment "\"Residential Group R occupancy, includes, among others, the use of a building or structure, or a portion thereof, for sleeping purposes when not classified when not classified as an Institutional Group I or when not regulated by the International Residential Code.\" (2024 IBC)" ;
                     rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                      "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                     rdfs:label "Residential Building" ;
                     <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                     <http://www.w3.org/2004/02/skos/core#example> "Hotels (transient), Motels (transient); Congregate living facilities (nontransient) with more than 16 occupants (Boarding houses (nontransient), Convents, Dormitories, Emergency services living quarters,Fraternities and sororities, Monasteries)" .


###  https://w3id.org/fedecom/lec-building#Room
:Room rdf:type owl:Class ;
      rdfs:comment "An enclosed space within a building, separated by walls, intended for a specific use or function." ;
      rdfs:label "Room" ;
      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#SecondaryCircuit
:SecondaryCircuit rdf:type owl:Class ;
                  rdfs:subClassOf :Circuit ;
                  rdfs:comment "Sub-distribution level that delivers power from the main board to floor or building-specific panels, often three-phase and protected by larger breakers." ;
                  rdfs:label "Secondary Circuit" ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#Site
:Site rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasParticipant ;
                        owl:someValuesFrom :Building
                      ] ;
      rdfs:comment "A distinct physical location where a Local Energy Community has infrastructure or assets (such as generation units, storage, or consumption points). A single LEC can operate across multiple sites within its network." ;
      rdfs:label "Site" ;
      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#StorageBuilding
:StorageBuilding rdf:type owl:Class ;
                 rdfs:subClassOf :Building ;
                 rdfs:comment "Storage Group S occupancy includes, among others, the use of a building or structure, or a portion thereof, for storage that is not classified as a hazardous occupancy" ;
                 rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                  "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                 rdfs:label "Storage Building" ;
                 <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" ;
                 <http://www.w3.org/2004/02/skos/core#example> "Storage facilities, Garages" .


###  https://w3id.org/fedecom/lec-building#TertiaryCircuit
:TertiaryCircuit rdf:type owl:Class ;
                 rdfs:subClassOf :Circuit ;
                 rdfs:comment "Final distribution stage, supplying end-use devices; typically single-phase 220 V circuits, each protected by its own breaker or RCD." ;
                 rdfs:label "Tertiary Circuit" ;
                 <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://w3id.org/fedecom/lec-building#UtilityMiscellaneousBuilding
:UtilityMiscellaneousBuilding rdf:type owl:Class ;
                              rdfs:subClassOf :Building ;
                              rdfs:comment "Buildings and structures of an accessory character and miscellaneous structures not classified in any specific occupancy. Shall be constructed, equipped and maintained to conform to the requirements of this code commensurate with the fire and life hazard incidental to their occupancy" ;
                              rdfs:isDefinedBy "2024 International Building Code (Ver. Aug 2025)" ,
                                               "https://codes.iccsafe.org/content/IBC2024V2.0/chapter-3-occupancy-classification-and-use" ;
                              rdfs:label "Utility Miscellaneous Building" ;
